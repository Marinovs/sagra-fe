# Frontend URL - Required for proper Open Graph images on WhatsApp/social media
# Set this to your production domain (e.g., https://yourdomain.com)
NEXT_PUBLIC_APP_URL=https://your-production-domain.com

# Backend API URL
NEXT_PUBLIC_BE_URI=https://your-backend-api.com

# Stripe (if using payment processing)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your-stripe-key

# Google Site Verification (optional)
GOOGLE_VERIFICATION_CODE=your-google-verification-code
